{
	"pattern": "*.volt",
	"testCommandPrefix": "//T ",
	"defaultCommands": [
		"//T run:volta -o %t %s",
		"//T run:%t"
	],
	"macros": {
		"expect-failure": [
			"//T retval:1",
			"//T run:volta -o %t %s"
		],
		"debug": [
			"//T run:volta -d -o %t %s",
			"//T run:%t"
		],
		"do-not-link": [
			"//T run:volta -c %s"
		]
	},
	"requiresAliases": {
		"windows": "msvc",
		"mac": "osx",
		"exceptions": "!windows"
	}
}
